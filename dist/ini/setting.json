{
    "version": "2.1.2",
    "seqCounter": {
        "encoding": "utf-8",
        "inputOptions": {
            "seqPath": "./seqs/",
            "seqExtensionName":".fasta",
            "encoding": "utf-8",
            "symbols": [">",">>"]
        },
        "outputOptions": {
            "resultPath": "./results/",
            "resultExtensionName": ".log",
            "encoding": "utf-8",
            "compare": true,
            "combineCompare": false,
            "extractSeq": true,
            "singleExtract": false,
            "extractExtensionName": ".fasta"
        },
        "constraintOptions": {
            "seqTypeList": "./ini/virusinfo.ini",
            "seqTypeCheck": true
        }
    }
}