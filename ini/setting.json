{
    "version": "2.0.3",
    "seqCounter": {
        "encoding": "utf-8",
        "inputOptions": {
            "seqPath": "./seqs/",
            "seqExtensionName":".txt",
            "encoding": "utf-8",
            "symbols": [">",">>"]
        },
        "outputOptions": {
            "resultPath": "./results/",
            "resultExtensionName": ".log",
            "encoding": "utf-8",
            "compare": true,
            "combineCompare": false
